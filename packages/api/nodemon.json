{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/",
    "dist/",
    "coverage/",
    "**/*.test.ts", "**/*.spec.ts"
  ],
  "watch": [
    "src/"
  ],
  "execMap": {
    "ts": "node -r ts-node/register"
  },
  "exec": "yarn start",
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts"
}
