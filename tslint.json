{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-empty-interface": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": [true, "dev"],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
